{
  "name": "@code-cli-sdk/claude",
  "version": "0.0.1",
  "description": "A minimal typescript library.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS=--conditions=development tsx src/index.ts",
    "example:claude": "NODE_OPTIONS=--conditions=development tsx example/claude-code-provider.ts",
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "test": "vitest run"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.7",
    "@anthropic-ai/sdk": "^0.71.2",
    "@code-cli-sdk/core": "workspace:*",
    "diff": "^8.0.3"
  }
}